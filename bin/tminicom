#!/bin/sh

usage() {
	echo "Target $1 not supported"
}

main() {
	case "${1}" in
		apu3)
			minicom -b 115000 \
				-D /dev/ttyUSB_APU3
			;;
		alix)
			minicom -b 38400 \
				-D /dev/ttyUSB_ALIX
			;;
		drno)
			minicom -b 115200 \
				-D /dev/ttyUSB_DRNO
			;;
		largo)
			minicom -b 115200 \
				-D /dev/ttyUSB_LARGO
			;;
		*)
			usage
			;;
	esac
}

main "$@"
